{
  "name": "react-zefhooks",
  "version": "1.1.6",
  "description": " A collection of react hooks.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/types/hooks/*.d.ts",
      "import": "./dist/esm/hooks/*.js",
      "require": "./dist/cjs/hooks/*.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "clean": "rm -rf dist",
    "build": "npm run clean && node esbuild.config.cjs && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HenryLF/react-zefhooks.git"
  },
  "author": "",
  "license": "free4ever",
  "bugs": {
    "url": "https://github.com/HenryLF/react-zefhooks/issues"
  },
  "homepage": "https://github.com/HenryLF/react-zefhooks#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "esbuild": "^0.25.9",
    "rollup": "^4.46.3"
  },
  "files": [
    "dist"
  ]
}